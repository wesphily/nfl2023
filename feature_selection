import xgboost as xgb
from sklearn.metrics import roc_auc_score
from sklearn.model_selection import train_test_split
from sklearn.preprocessing import MinMaxScaler
import pandas as pd
import numpy as np





## Convert excels to dataframs ##
rushing2022 = pd.read_csv(r'D:\NFL\stats\rushing2022.csv')
passing2022 = pd.read_csv(r'D:\NFL\stats\passing2022.csv')
receiving2022 = pd.read_csv(r'D:\NFL\stats\receiving2022.csv')
rushing_defense2022 = pd.read_csv(r'D:\NFL\stats\rushing_defense2022.csv')
passing_defense2022 = pd.read_csv(r'D:\NFL\stats\passing_defense2022.csv')


## combine dataframes ##
team_data2022 = rushing2022.append([passing2022,receiving2022,rushing_defense2022,passing_defense2022])
list(team_data2022)
team_data2022 = team_data2022.groupby(['team_name'])[['total_rushing_attempts_o','total_rushing_yards_o','average_rush_yards_per_attempt_o','total_rush_td_o','total_rush_fumbles_o','total_passing_attempts_o','total_passing_completions_o','pass_completion_percentage_o','average_yards_per_pass_attempt_o','total_passing_yards_o','total_passing_td_o','total_passing_interceptions_o','passer_rating_o','total_sacks_o','total_yards_lost_sack_o','total_receiving_receptions_o','total_receiving_yards_o','average_receiving_yards_o','total_receiving_td_o','total_reciving_fumbles_o','def_rush_attempts','def_rush_yards','def_rush_yards_per_carry','def_rush_td','def_pass_attempt','def_pass_complete','def_pass_complete_percent','def_pass_yards_attempt','def_pass_yards','def_pass_td','def_pass_int','def_pass_first_downs','def_pass_sack']].sum()
team_data2022 = team_data2022.reset_index(drop=False)


## I drop these features because they rarely contribute a lot to a successful prediction ##
#team_data2022.drop(['longest_interception_return_d', 'longest_pass_td_o',  'longest_receiving_distance_o',  'longest_rush_o'],axis=1,inplace=True)

########## Data Cleaning ##############
### I chose the bin sizes based on domain knowledge. I increased the number of bins for features that have a bigger impact on a particular section ##
### The goal of each section was to assign a value to what contributes to a section being successful and then subtract the sum of what makes a section unsuccessful. ##
### I binned the final result and then created a new column for each bin. ##
### The idea is that low number bins are bad and high number bins are good. ###
### Once the final bin is determined for each section I drop the original features that helped build the final bin ###


####Offensive Passing Performance######
team_data2022['pass_completion_percentage_o'], pass_completion_percentage_obins=pd.qcut(team_data2022.pass_completion_percentage_o, q=4, labels=False, retbins=True, precision=3, duplicates='raise')
team_data2022['passer_rating_o'], passer_rating_obins=pd.qcut(team_data2022.passer_rating_o, q=7, labels=False, retbins=True, precision=3, duplicates='raise')
team_data2022['passing_average_completion'] = team_data2022['total_passing_completions_o'] / team_data2022['total_passing_attempts_o']
team_data2022['passing_average_completion'], passing_average_completionbins=pd.qcut(team_data2022.passing_average_completion, q=4, labels=False, retbins=True, precision=3, duplicates='raise')
team_data2022.drop(['total_passing_attempts_o', 'total_passing_completions_o'],axis=1,inplace=True)
team_data2022['total_passing_interceptions_o'], total_passing_interceptions_obins=pd.qcut(team_data2022.total_passing_interceptions_o, q=4, labels=False, retbins=True, precision=3, duplicates='raise')
team_data2022['total_passing_td_o'], total_passing_td_obins=pd.qcut(team_data2022.total_passing_td_o, q=8, labels=False, retbins=True, precision=3, duplicates='raise')
team_data2022['total_passing_yards_o'], total_passing_yards_obins=pd.qcut(team_data2022.total_passing_yards_o, q=8, labels=False, retbins=True, precision=3, duplicates='raise')
team_data2022['average_yards_per_pass_attempt_o'], average_yards_per_pass_attempt_obins=pd.qcut(team_data2022.average_yards_per_pass_attempt_o, q=4, labels=False, retbins=True, precision=3, duplicates='raise')
team_data2022['total_sacks_o'], total_sacks_obins=pd.qcut(team_data2022.total_sacks_o, q=4, labels=False, retbins=True, precision=3, duplicates='raise')
team_data2022['total_yards_lost_sack_o'], total_yards_lost_sack_obins=pd.qcut(team_data2022.total_yards_lost_sack_o, q=4, labels=False, retbins=True, precision=3, duplicates='raise')
team_data2022['passing_performance']= (team_data2022['average_yards_per_pass_attempt_o'] + team_data2022['pass_completion_percentage_o']+ team_data2022['passer_rating_o'] + team_data2022['passing_average_completion'] + team_data2022['total_passing_td_o'] + team_data2022['total_passing_yards_o']) - (team_data2022['total_passing_interceptions_o']+ team_data2022['total_sacks_o'] + team_data2022['total_yards_lost_sack_o'])
team_data2022.drop(['average_yards_per_pass_attempt_o','total_passing_td_o','pass_completion_percentage_o', 'passer_rating_o', 'passing_average_completion', 'total_passing_yards_o', 'total_passing_interceptions_o', 'total_sacks_o', 'total_yards_lost_sack_o'],axis=1,inplace=True)
team_data2022['passing_performance'], passing_performancebins=pd.qcut(team_data2022.passing_performance, q=5, labels=False, retbins=True, precision=3, duplicates='raise')
team_data2022['passing_performance0']=0
team_data2022.loc[(team_data2022['passing_performance']==0), 'passing_performance0']=1
team_data2022['passing_performance1']=0
team_data2022.loc[(team_data2022['passing_performance']==1), 'passing_performance1']=1
team_data2022['passing_performance2']=0
team_data2022.loc[(team_data2022['passing_performance']==2), 'passing_performance2']=1
team_data2022['passing_performance3']=0
team_data2022.loc[(team_data2022['passing_performance']==3), 'passing_performance3']=1
team_data2022.drop(['passing_performance'],axis=1,inplace=True)

####Def Interception Performance####
team_data2022['def_pass_int'], def_pass_intbins=pd.qcut(team_data2022.def_pass_int, q=4, labels=False, retbins=True, precision=3, duplicates='raise')
team_data2022['defense_interception_performance0']=0
team_data2022.loc[(team_data2022['def_pass_int']==0), 'defense_interception_performance0']=1
team_data2022['defense_interception_performance1']=0
team_data2022.loc[(team_data2022['def_pass_int']==1), 'defense_interception_performance1']=1
team_data2022['defense_interception_performance2']=0
team_data2022.loc[(team_data2022['def_pass_int']==2), 'defense_interception_performance2']=1
team_data2022['defense_interception_performance3']=0
team_data2022.loc[(team_data2022['def_pass_int']==3), 'defense_interception_performance3']=1
team_data2022.drop(['def_pass_int'],axis=1,inplace=True)

####Def Rushing Performance####
team_data2022['def_rush_attempts'], def_rush_attemptsbins=pd.qcut(team_data2022.def_rush_attempts, q=4, labels=False, retbins=True, precision=3, duplicates='raise')
team_data2022['def_rush_yards'], def_rush_yardsbins=pd.qcut(team_data2022.def_rush_yards, q=4, labels=False, retbins=True, precision=3, duplicates='raise')
team_data2022['def_rush_yards_per_carry'], def_rush_yards_per_carrybins=pd.qcut(team_data2022.def_rush_yards_per_carry, q=4, labels=False, retbins=True, precision=3, duplicates='raise')
team_data2022['def_rush_td'], def_rush_tdbins=pd.qcut(team_data2022.def_rush_td, q=4, labels=False, retbins=True, precision=3, duplicates='raise')
team_data2022['def_rush_performance']= (team_data2022['def_rush_attempts'] + team_data2022['def_rush_yards']+ team_data2022['def_rush_yards_per_carry'] + team_data2022['def_rush_td'])
team_data2022.drop(['def_rush_attempts','def_rush_yards','def_rush_yards_per_carry', 'def_rush_td'],axis=1,inplace=True)
team_data2022['def_rush_performance'], def_rush_performancebins=pd.qcut(team_data2022.def_rush_performance, q=4, labels=False, retbins=True, precision=3, duplicates='raise')
team_data2022['def_rush_performance0']=0
team_data2022.loc[(team_data2022['def_rush_performance']==0), 'def_rush_performance0']=1
team_data2022['def_rush_performance1']=0
team_data2022.loc[(team_data2022['def_rush_performance']==1), 'def_rush_performance1']=1
team_data2022['def_rush_performance2']=0
team_data2022.loc[(team_data2022['def_rush_performance']==2), 'def_rush_performance2']=1
team_data2022['def_rush_performance3']=0
team_data2022.loc[(team_data2022['def_rush_performance']==3), 'def_rush_performance3']=1
team_data2022.drop(['def_rush_performance'],axis=1,inplace=True)

####Def Passing Performance####
team_data2022['def_pass_attempt'], def_pass_attemptbins=pd.qcut(team_data2022.def_pass_attempt, q=4, labels=False, retbins=True, precision=3, duplicates='raise')
team_data2022['def_pass_complete'], def_pass_completebins=pd.qcut(team_data2022.def_pass_complete, q=4, labels=False, retbins=True, precision=3, duplicates='raise')
team_data2022['def_pass_yards_attempt'], def_pass_yards_attemptbins=pd.qcut(team_data2022.def_pass_yards_attempt, q=4, labels=False, retbins=True, precision=3, duplicates='raise')
team_data2022['def_pass_yards'], def_pass_yardsbins=pd.qcut(team_data2022.def_pass_yards, q=4, labels=False, retbins=True, precision=3, duplicates='raise')
team_data2022['def_pass_td'], def_pass_tdbins=pd.qcut(team_data2022.def_pass_td, q=4, labels=False, retbins=True, precision=3, duplicates='raise')
team_data2022['def_pass_performance']= (team_data2022['def_pass_attempt'] + team_data2022['def_pass_complete']+ team_data2022['def_pass_yards_attempt'] + team_data2022['def_pass_yards'] + team_data2022['def_pass_td'])
team_data2022.drop(['def_pass_complete_percent','def_pass_first_downs','def_pass_attempt','def_pass_complete','def_pass_yards_attempt', 'def_pass_yards', 'def_pass_td'],axis=1,inplace=True)
team_data2022['def_pass_performance'], def_pass_performancebins=pd.qcut(team_data2022.def_pass_performance, q=4, labels=False, retbins=True, precision=3, duplicates='raise')
team_data2022['def_pass_performance0']=0
team_data2022.loc[(team_data2022['def_pass_performance']==0), 'def_pass_performance0']=1
team_data2022['def_pass_performance1']=0
team_data2022.loc[(team_data2022['def_pass_performance']==1), 'def_pass_performance1']=1
team_data2022['def_pass_performance2']=0
team_data2022.loc[(team_data2022['def_pass_performance']==2), 'def_pass_performance2']=1
team_data2022['def_pass_performance3']=0
team_data2022.loc[(team_data2022['def_pass_performance']==3), 'def_pass_performance3']=1
team_data2022.drop(['def_pass_performance'],axis=1,inplace=True)

team_data2022['def_pass_sack'], def_pass_sackbins=pd.qcut(team_data2022.def_pass_sack, q=4, labels=False, retbins=True, precision=3, duplicates='raise')
team_data2022['sack_performance0']=0
team_data2022.loc[(team_data2022['def_pass_sack']==0), 'sack_performance0']=1
team_data2022['sack_performance1']=0
team_data2022.loc[(team_data2022['def_pass_sack']==1), 'sack_performance1']=1
team_data2022['sack_performance2']=0
team_data2022.loc[(team_data2022['def_pass_sack']==2), 'sack_performance2']=1
team_data2022['sack_performance3']=0
team_data2022.loc[(team_data2022['def_pass_sack']==3), 'sack_performance3']=1
team_data2022.drop(['def_pass_sack'],axis=1,inplace=True)

team_data2022['average_rush_yards_per_attempt_o'], average_rush_yards_per_attempt_obins=pd.qcut(team_data2022.average_rush_yards_per_attempt_o, q=2, labels=False, retbins=True, precision=3, duplicates='raise')
team_data2022['total_rush_fumbles_o'], total_rush_fumbles_obins=pd.qcut(team_data2022.total_rush_fumbles_o, q=4, labels=False, retbins=True, precision=3, duplicates='raise')
team_data2022['total_rush_td_o'], total_rush_td_obins=pd.qcut(team_data2022.total_rush_td_o, q=4, labels=False, retbins=True, precision=3, duplicates='raise')
team_data2022['total_rushing_attempts_o'], total_rushing_attempts_obins=pd.qcut(team_data2022.total_rushing_attempts_o, q=4, labels=False, retbins=True, precision=3, duplicates='raise')
team_data2022['total_rushing_yards_o'], total_rushing_yards_obins=pd.qcut(team_data2022.total_rushing_yards_o, q=4, labels=False, retbins=True, precision=3, duplicates='raise')
team_data2022['rushing_performance']= (team_data2022['average_rush_yards_per_attempt_o'] + team_data2022['average_rush_yards_per_attempt_o'] + team_data2022['total_rush_td_o'] + team_data2022['total_rushing_attempts_o'] + team_data2022['total_rushing_yards_o'])
team_data2022.drop(['average_rush_yards_per_attempt_o', 'average_rush_yards_per_attempt_o', 'total_rush_fumbles_o', 'total_rush_td_o', 'total_rushing_attempts_o', 'total_rushing_yards_o'],axis=1,inplace=True)

team_data2022['rushing_performance'], rushing_performancebins=pd.qcut(team_data2022.rushing_performance, q=4, labels=False, retbins=True, precision=3, duplicates='raise')
team_data2022['rushing_performance0']=0
team_data2022.loc[(team_data2022['rushing_performance']==0), 'rushing_performance0']=1
team_data2022['rushing_performance1']=0
team_data2022.loc[(team_data2022['rushing_performance']==1), 'rushing_performance1']=1
team_data2022['rushing_performance2']=0
team_data2022.loc[(team_data2022['rushing_performance']==2), 'rushing_performance2']=1
team_data2022['rushing_performance3']=0
team_data2022.loc[(team_data2022['rushing_performance']==3), 'rushing_performance3']=1
team_data2022.drop(['rushing_performance'],axis=1,inplace=True)



team_data2022['total_receiving_receptions_o'], total_receiving_receptions_obins=pd.qcut(team_data2022.total_receiving_receptions_o, q=4, labels=False, retbins=True, precision=3, duplicates='raise')
team_data2022['total_receiving_td_o'], total_receiving_td_obins=pd.qcut(team_data2022.total_receiving_td_o, q=4, labels=False, retbins=True, precision=3, duplicates='raise')
team_data2022['total_receiving_yards_o'], total_receiving_yards_obins=pd.qcut(team_data2022.total_receiving_yards_o, q=4, labels=False, retbins=True, precision=3, duplicates='raise')
team_data2022['total_reciving_fumbles_o'], total_reciving_fumbles_obins=pd.qcut(team_data2022.total_reciving_fumbles_o, q=4, labels=False, retbins=True, precision=3, duplicates='raise')
team_data2022['average_receiving_yards_o'], average_receiving_yards_obins=pd.qcut(team_data2022.average_receiving_yards_o, q=4, labels=False, retbins=True, precision=3, duplicates='raise')
team_data2022['receiving_performance']=(team_data2022['average_receiving_yards_o'] + team_data2022['total_receiving_receptions_o'] + team_data2022['total_receiving_td_o'] + team_data2022['total_receiving_yards_o'])
team_data2022.drop(['average_receiving_yards_o', 'total_receiving_receptions_o', 'total_receiving_td_o', 'total_receiving_yards_o', 'total_reciving_fumbles_o'],axis=1,inplace=True)

team_data2022['receiving_performance'], receiving_performancebins=pd.qcut(team_data2022.receiving_performance, q=4, labels=False, retbins=True, precision=3, duplicates='raise')
team_data2022['receiving_performance0']=0
team_data2022.loc[(team_data2022['receiving_performance']==0), 'receiving_performance0']=1
team_data2022['receiving_performance1']=0
team_data2022.loc[(team_data2022['receiving_performance']==1), 'receiving_performance1']=1
team_data2022['receiving_performance2']=0
team_data2022.loc[(team_data2022['receiving_performance']==2), 'receiving_performance2']=1
team_data2022['receiving_performance3']=0
team_data2022.loc[(team_data2022['receiving_performance']==3), 'receiving_performance3']=1
team_data2022.drop(['receiving_performance'],axis=1,inplace=True)



#########################    2022 Games    #########################


#### Philadelphia Games ####
game1_philadelphia = team_data2022.loc[(team_data2022['team_name']=='Eagles')]
game1_opponent = team_data2022.loc[(team_data2022['team_name']=='Lions')]
game1_philadelphia_end = game1_philadelphia.drop(['team_name'],axis=1,inplace=False).sub(game1_opponent.drop(['team_name'],axis=1,inplace=False).squeeze())
game1_philadelphia_end['home_advantage']=0
game1_philadelphia_end['team_name']='Eagles'
game1_philadelphia_end['win']=1

game2_philadelphia = team_data2022.loc[(team_data2022['team_name']=='Eagles')]
game2_opponent = team_data2022.loc[(team_data2022['team_name']=='Vikings')]
game2_philadelphia_end = game2_philadelphia.drop(['team_name'],axis=1,inplace=False).sub(game2_opponent.drop(['team_name'],axis=1,inplace=False).squeeze())
game2_philadelphia_end['home_advantage']=1
game2_philadelphia_end['team_name']='Eagles'
game2_philadelphia_end['win']=1

